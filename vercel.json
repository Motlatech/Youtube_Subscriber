{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb",
                "timeout": 60
            }
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/server.js"
        }
    ]
}
